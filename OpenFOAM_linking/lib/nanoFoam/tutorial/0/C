FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "25";
    object      C;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0.0; 

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 2.1297E-05;
    }
    wall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
}


